<?xml version="1.0" encoding="utf-8"?>
<Containers>
  <TemplateURL>https://raw.githubusercontent.com/binhex/docker-templates/master/binhex/pycharm.xml</TemplateURL>
  <Date>2019-05-31</Date>
  <Beta>False</Beta>
  <Category>Download:</Category>
  <Name>c1tas-flexget</Name>
  <Project>https://flexget.tv/</Project>
  <Changes>
    - C1tas Initial Release
  </Changes>
  <Overview>
    The flexget download rss
  </Overview>
  <Description>
    The c1tas custom flexget download server
  </Description>
  <Registry>https://hub.docker.com/r/c1tas/flexget</Registry>
  <GitHub>https://github.com/c1tas</GitHub>
  <Repository>c1tas/flexget</Repository>
  <Support>https://github.com/c1tas</Support>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>5050</HostPort>
        <ContainerPort>5050</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Environment>
    <Variable>
      <Name>WEB_PASSWD</Name>
      <Value>lulu501522</Value>
    </Variable>
    <Variable>
      <Name>FLEXGET_LOG_LEVEL</Name>
      <Value>debug</Value>
    </Variable>
  </Environment>
  <Data>
    <Volume>
      <HostDir>/mnt/user/appdata/c1tas-flexget</HostDir>
      <ContainerDir>/config</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/user/download/</HostDir>
      <ContainerDir>/downloads</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <WebUI>http://[IP]:[PORT:5050]/index.html</WebUI>
  <Banner></Banner>
  <Icon>https://avatars2.githubusercontent.com/u/17483320?s=400</Icon>
  <ExtraParams></ExtraParams>
</Containers>